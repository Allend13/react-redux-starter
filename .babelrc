{
  "presets": [
    ["es2015", {"modules": false}],
    "stage-2",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-es2015-modules-commonjs",
    ["module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./components",
          "routes": "./routes",
          "actions": "./actions",
          "constants": "./constants",
          "reducers": "./reducers/",
          "less": "./less"
        }
      }
    ]
  ]
}
